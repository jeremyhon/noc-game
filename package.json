{
  "name": "noc-game",
  "version": "0.1.0",
  "homepage": "https://jeremyhon.github.io/noc-game",
  "private": true,
  "dependencies": {
    "chance": "^1.0.9",
    "classnames": "^2.2.5",
    "express": "^4.15.3",
    "material-ui": "^0.18.6",
    "npm-run-all": "^4.0.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "enzyme": "^2.8.2",
    "gh-pages": "^1.0.0",
    "jest-enzyme": "^3.2.0",
    "node-sass-chokidar": "^0.0.3",
    "react-scripts": "^1.0.7",
    "react-test-renderer": "^15.5.4"
  },
  "scripts": {
    "build-css": "node ./node_modules/.bin/node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node ./node_modules/.bin/node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "NODE_PATH=./src npm-run-all -p watch-css start-js",
    "build": "NODE_PATH=./src npm run build-css && NODE_PATH=./src react-scripts build",
    "test": "npm run build-css && NODE_PATH=./src react-scripts test --env=jsdom",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  }
}
